{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["ESNext"],
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"resolveJsonModule": true,
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"verbatimModuleSyntax": true,
		"erasableSyntaxOnly": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
